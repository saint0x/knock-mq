{
  "name": "knock-mq",
  "version": "0.0.1",
  "description": "Production-grade message queue implementation.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./queue": {
      "import": "./dist/queue.js",
      "types": "./dist/queue.d.ts"
    },
    "./storage": {
      "import": "./dist/storage.js",
      "types": "./dist/storage.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "storage.examples.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "bun run tests/server.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "message-queue",
    "queue",
    "job-queue",
    "typescript",
    "production",
    "distributed-systems",
    "performance",
    "circuit-breaker",
    "retry",
    "dead-letter-queue"
  ],
  "author": "saint0x",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saint0x/knock-mq.git"
  },
  "homepage": "https://github.com/saint0x/knock-mq",
  "bugs": {
    "url": "https://github.com/saint0x/knock-mq/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0",
    "hono": "^3.12.0",
    "@hono/node-server": "^1.8.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
